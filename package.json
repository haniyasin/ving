{
    "name": "ving",
    "description": "An opinionated web services starter for Nuxt3 that provides REST and per-field privileges out of the box.",
    "private": true,
    "type": "commonjs",
    "scripts": {
        "build": "nuxt build",
        "dev": "nuxt dev",
        "generate": "nuxt generate",
        "preview": "nuxt preview",
        "postinstall": "nuxt prepare",
        "tests": "vitest",
        "ving:gentables": "esbuild server/vingschema/gentable.ts --platform=node --bundle --log-level=warning --outfile=.output/server/vingschema/gentable.js && node .output/server/vingschema/gentable.js",
        "drizzle:gen": "drizzle-kit generate:mysql --out ./server/drizzle/migrations --schema server/drizzle/schema",
        "drizzle:migrate": "esbuild server/drizzle/migrate.ts --platform=node --bundle --log-level=warning --outfile=.output/server/drizzle/migrate.js && node .output/server/drizzle/migrate.js"
    },
    "devDependencies": {
        "@nuxt/content": "^2.5.2",
        "@nuxt/test-utils": "^3.2.2",
        "@types/bcryptjs": "^2.4.2",
        "@types/lodash": "^4.14.191",
        "@types/luxon": "^3.2.0",
        "@types/uuid": "^9.0.1",
        "drizzle-kit": "^0.17.0",
        "nuxt": "^3.2.2",
        "nuxt-icon": "^0.3.3",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5",
        "vitest": "^0.28.5"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^6.3.0",
        "@fortawesome/free-brands-svg-icons": "^6.3.0",
        "@fortawesome/free-regular-svg-icons": "^6.3.0",
        "@fortawesome/free-solid-svg-icons": "^6.3.0",
        "@fortawesome/pro-duotone-svg-icons": "^6.3.0",
        "@fortawesome/pro-light-svg-icons": "^6.3.0",
        "@fortawesome/pro-regular-svg-icons": "^6.3.0",
        "@fortawesome/pro-solid-svg-icons": "^6.3.0",
        "@fortawesome/pro-thin-svg-icons": "^6.3.0",
        "@fortawesome/sharp-regular-svg-icons": "^6.3.0",
        "@fortawesome/sharp-solid-svg-icons": "^6.3.0",
        "@fortawesome/vue-fontawesome": "^3.0.3",
        "@keyv/redis": "^2.5.5",
        "@pinia/nuxt": "^0.4.7",
        "bcryptjs": "^2.4.3",
        "drizzle-orm": "^0.22.0-7239945",
        "keyv": "^4.5.2",
        "lodash": "^4.17.21",
        "luxon": "^3.2.1",
        "mysql2": "^2.3.3",
        "pinia": "^2.0.32",
        "primeflex": "^3.3.0",
        "primeicons": "^6.0.1",
        "primevue": "^3.23.0",
        "uuid": "^9.0.0",
        "zod": "^3.21.4"
    }
}